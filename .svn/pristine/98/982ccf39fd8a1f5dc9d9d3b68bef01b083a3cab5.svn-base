@charset "utf-8";
/* CSS Document */
/*css reset start*/
html {
	background-color:#fff;
	margin:0px auto;
	padding:0px;
	height:100%;
}
body {
	font-size:13px;
	background-color:#fff;
	font-family:Microsoft Yahei, Arial;
	margin:0 auto;
	padding:0;
	color:#3c3c3c;
	line-height:22px;
	width:100%; 
	height:100%;
}
button, input, select, textarea {
	font-family:Microsoft Yahei, Arial;
	border:none;
	color:#3c3c3c;
}
input {
	margin:0;
	padding:0;
	vertical-align:middle;
}
a {
	color:#f25353;
	text-decoration:none;
	cursor:pointer;
}
a:hover {
	color:#de2828;
	text-decoration:none;
}
dl, dt, dd, ul, ol, p, dl {
	margin:0;
	padding:0
}
/*ul, ol, dl {
	height:100%;
	overflow:hidden;
}*/
li {
	list-style:none
}
img {
	border:none;
	vertical-align:middle;
/*	width:100%;*/
}
h1, h2, h3, h4 {
	margin:0;
	padding:0;
	font-weight: normal;
}
button {
	cursor:pointer;
	outline:none;
}
/*css reset end*/
/*column*/
.content {
	width:100%; 
	height:100%; 
	overflow:hidden;
}
.top {
	width:100%; 
	height:80px;
	background:#2670b4;
	overflow:hidden;
}
.column {
	width:100%;
}
.sidebar {
	width:100%;
	overflow:hidden;
	height:98%;
}
.subcolu {
	width:100%;
	height:35%;
	overflow:hidden;
}

